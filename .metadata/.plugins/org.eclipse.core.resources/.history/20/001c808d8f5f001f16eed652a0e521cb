package newproject1;
import java.util.*;

public class newclass {
	
	int empid=1222;
	String name="Global"
	
	public void sample1(int empid, String name) {
		System.out.println("Global variable The id is "+this.empid+" the name is "+this.name);
		System.out.println("Local variable The id is "+empid+" the name is "+name);
	}
	
	public static void m2(int a){
		System.out.println("The number is "+a);
	}
	
public static void main(String args[]) {
		
		int a;
		Scanner Sc = new Scanner(System.in);
		System.out.println("Enter the number");
		a=Sc.nextInt();
		
		newclass n1=new newclass();
		n1.sample1(12345,"Manju");
		
					
	}
}
