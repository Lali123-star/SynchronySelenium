package newproject1;
import java.util.*;

public class newclass {
public static void main(String args[]) {
		
		int a;
		Scanner Sc = new Scanner(System.in);
		System.out.println("Enter the number");
		a=Sc.nextInt();
		
		/*while(a<6) {
			System.out.println(a);
			a++;
		
		}*/
		int res=1;
		for( int i=a; i>0;i--) {
			
			
			res=res*i;
			
			
			
			
		}
		System.out.println("The factorial is " + res);
					
	}
}
